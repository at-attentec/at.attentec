<tr>
  <th><%= User.human_attribute_name(:title) %>:</th>
  <td><%=h @user.title %></td>
</tr>
<tr>
  <th><%= User.human_attribute_name(:degree) %>:</th>
  <td><%=h @user.degree %></td>
</tr>
<tr>
  <th><%= User.human_attribute_name(:client) %>:</th>
  <td><%=h @user.client %></td>
</tr>
<tr>
  <th><%= User.human_attribute_name(:linkedin_url) %>:</th>
  <% if !@user.linkedin_url.blank? %>
    <td><%= link_to @user.linkedin_url, @user.linkedin_url %></td>
  <% end %>
</tr>
